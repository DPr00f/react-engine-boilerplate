{
  "name": "react-engine-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for react-engine",
  "main": "index.js",
  "scripts": {
    "test": "BABEL_JEST_STAGE=0 jest --verbose --captureExceptions",
    "start": "node dist/server.js"
  },
  "keywords": [
    "boilerplate",
    "react",
    "react-engine",
    "engine",
    "es6",
    "babel"
  ],
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "author": "Joao Lopes <jocolopes@gmail.com> (http://joaolopes.info)",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^5.3.0",
    "babelify": "^6.1.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "gulp-browserify": "^0.5.1",
    "gulp-ext-replace": "^0.2.0",
    "gulp-react": "^3.0.1",
    "gulp-sass": "^2.0.1",
    "jest-cli": "^0.4.13",
    "reactify": "^1.1.1",
    "require-globify": "^1.2.1"
  },
  "dependencies": {
    "express": "^4.12.4",
    "react": "^0.13.3",
    "react-engine": "^1.6.0",
    "react-router": "^0.13.3"
  }
}
